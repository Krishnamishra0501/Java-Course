public class Student {
    int rollno;
    String name;
    String branch;
}

public class Stdmain {

	public static void main(String[] args) {
		Student s1=new Student();
		s1.rollno=100;
		s1.name="ABC";
		s1.branch = "CSE";
		System.out.println(s1.rollno);
		System.out.println(s1.name);
		
		// TODO Auto-generated method stub
	}

}
